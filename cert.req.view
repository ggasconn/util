#!/bin/bash

################################################################################
# cert.req.view
################################################################################
# view a CSR
################################################################################

if [ $# -eq 0 ]
then
  printf "\n\n\nUsage:\n\t$0 csr...\n\n"
  exit 1
fi

for CSR in $*
do

  openssl req -in $CSR -noout -text

done
